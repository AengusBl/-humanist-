        <when state="stroke" output="-"/>
        <when state="tail" output="˞"/>
        <when state="tilde" output="˜"/>
        <when state="tildeUnder" output="˷"/>
        <when state="wedge" output="ˇ"/>
        <when state="wedgeUnder" output=" ̬"/>
        <!-- GREEK -->
        <when state="ᵍacute" output="´"/>
        <when state="ᵍacuteIota" output="´ͅ"/>
        <when state="ᵍacuteTrema" output="΅"/>
        <when state="ᵍasper" output=";"/>
        <when state="ᵍasperAcute" output="῞"/>
        <when state="ᵍasperAcuteIota" output="῞ͅ"/>
        <when state="ᵍasperCircumflex" output="῟"/>
        <when state="ᵍasperCircumflexIota" output="῟ͅ"/>
        <when state="ᵍasperGrave" output="῝"/>
        <when state="ᵍasperGraveIota" output="῝ͅ"/>
        <when state="ᵍasperIota" output="῾ͅ"/>
        <when state="ᵍbreve" output="˘"/>
