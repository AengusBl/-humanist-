            <!-- Greek numeral sign -->
        </action>
        <action id="ᵍasper">
            <when state="none" next="ᵍasper"/>
            <when state="ᵍiota" next="ᵍasperIota"/>
        </action>
        <action id="ᵍasperAcute">
            <when state="none" next="ᵍasperAcute"/>
            <when state="ᵍiota" next="ᵍasperAcuteIota"/>
        </action>
        <action id="ᵍasperCircumflex">
            <when state="none" next="ᵍasperCircumflex"/>
            <when state="ᵍiota" next="ᵍasperCircumflexIota"/>
        </action>
        <action id="ᵍasperGrave">
            <when state="none" next="ᵍasperGrave"/>
            <when state="ᵍiota" next="ᵍasperGraveIota"/>
        </action>
        <action id="ᵍbreve">
            <when state="none" next="ᵍbreve"/>
        </action>
        <action id="ᵍcircumflex">
            <when state="none" next="ᵍcircumflex"/>
            <when state="ᵍiota" next="ᵍcircumflexIota"/>
            <when state="ᵍtrema" next="ᵍcircumflexTrema"/>
        </action>
        <action id="ᵍcircumflexTrema">
            <when state="none" next="ᵍcircumflexTrema"/>
        </action>
        <action id="ᵍgrave">
            <when state="none" next="ᵍgrave"/>
            <when state="ᵍiota" next="ᵍgraveIota"/>
            <when state="ᵍtrema" next="ᵍgraveTrema"/>
        </action>
        <action id="ᵍgraveTrema">
            <when state="none" next="ᵍgraveTrema"/>
        </action>
        <action id="ᵍiota">
            <when state="none" next="ᵍiota"/>
        </action>
        <action id="ᵍlenis">
            <when state="none" next="ᵍlenis"/>
            <when state="ᵍiota" next="ᵍlenisIota"/>
        </action>
        <action id="ᵍlenisAcute">
            <when state="none" next="ᵍlenisAcute"/>
            <when state="ᵍiota" next="ᵍlenisAcuteIota"/>
        </action>
        <action id="ᵍlenisCircumflex">
            <when state="none" next="ᵍlenisCircumflex"/>
            <when state="ᵍiota" next="ᵍlenisCircumflexIota"/>
        </action>
        <action id="ᵍlenisGrave">
            <when state="none" next="ᵍlenisGrave"/>
            <when state="ᵍiota" next="ᵍlenisGraveIota"/>
        </action>
        <action id="ᵍmacron">
            <when state="none" next="ᵍmacron"/>
        </action>
        <action id="ᵍperiod">
            <when state="none" output="."/>
            <when state="ᵍacute" output="́"/>
            <when state="ᵍacuteTrema" output="᷁"/>
            <when state="ᵍasper" output="̔"/>
            <when state="ᵍbreve" output="̆"/>
            <when state="ᵍcircumflex" output="͂"/>
            <when state="ᵍgrave" output="̀"/>
            <when state="ᵍgraveTrema" output="᷀"/>
            <when state="ᵍlenis" output="̓"/>
            <when state="ᵍmacron" output="̄"/>
            <when state="ᵍtrema" output="̈"/>
        </action>
        <action id="ᵍquote">
            <when state="none" output="&#x0022;"/>
            <!-- Latin quotation m. -->
            <when state="ᵍlenis" output="?"/>
        </action>
        <action id="ᵍtrema">
            <when state="none" next="ᵍtrema"/>
        </action>
        <!-- MYCENAEAN -->
        <action id="ᵐV">
            <when state="none" next="ᵐV"/>
        </action>
        <!-- GREEK LOWERCASE -->
        <action id="ᵐd">
            <when state="none" next="ᵐd"/>
        </action>
        <action id="ᵐj">
            <when state="none" next="ᵐj"/>
        </action>
        <action id="ᵐk">
            <when state="none" next="ᵐk"/>
        </action>
        <action id="ᵐm">
            <when state="none" next="ᵐm"/>
        </action>
        <action id="ᵐn">
            <when state="none" next="ᵐn"/>
        </action>
        <action id="ᵐp">
            <when state="none" next="ᵐp"/>
        </action>
        <action id="ᵐq">
            <when state="none" next="ᵐq"/>
        </action>
        <action id="ᵐr">
            <when state="none" next="ᵐr"/>
        </action>
        <action id="ᵐs">
            <when state="none" next="ᵐs"/>
        </action>
        <action id="ᵐt">
            <when state="none" next="ᵐt"/>
        </action>
        <action id="ᵐw">
            <when state="none" next="ᵐw"/>
        </action>
        <action id="ᵐz">
            <when state="none" next="ᵐz"/>
        </action>
        <action id="“">
            <when state="none" output="“"/>
            <!-- l. smart double quote -->
            <when state="ipa" output="ʽ"/>
            <!-- modif. l. reversed comma -->
            <when state="posUnder" output="᪸"/>
        </action>
    </actions>
    <terminators>
        <when state="acute" output="ˊ"/>
        <when state="acuteDouble" output="˝"/>
        <when state="arch" output=" ̑"/>
        <when state="archUnder" output=" ̯"/>
        <when state="breve" output="˘"/>
        <when state="cedilla" output="¸"/>
        <when state="circumflex" output="ˆ"/>
        <when state="circumflexUnder" output="ꞈ"/>
        <when state="commaUnder" output=","/>
        <when state="diaeresis" output="¨"/>
        <when state="dot" output="˙"/>
        <when state="dotUnder" output="."/>
        <when state="grave" output="ˋ"/>
        <when state="graveDouble" output="˵"/>
        <when state="ipa" output="ə"/>
        <when state="macron" output="ˉ"/>
        <when state="macronUnder" output="ˍ"/>
        <when state="ogonek" output="˛"/>
        <when state="posOver" output="⁺"/>
        <when state="posSub" output="₋"/>
        <when state="posSuper" output="⁻"/>
        <when state="posUnder" output="₊"/>
        <when state="ring" output="˚"/>
