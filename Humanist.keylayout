        <when state="ᵍlenisIota" output="᾿ͅ"/>
        <when state="ᵍmacron" output="¯"/>
        <when state="ᵍtrema" output="¨"/>
        <!-- MYCENAEAN -->
        <when state="ᵐV" output="𐀀"/>
        <when state="ᵐd" output="𐀅"/>
        <when state="ᵐj" output="𐀊"/>
        <when state="ᵐk" output="𐀏"/>
        <when state="ᵐm" output="𐀔"/>
        <when state="ᵐn" output="𐀙"/>
        <when state="ᵐp" output="𐀞"/>
        <when state="ᵐq" output="𐀣"/>
        <when state="ᵐr" output="𐀨"/>
        <when state="ᵐs" output="𐀭"/>
        <when state="ᵐt" output="𐀲"/>
        <when state="ᵐw" output="𐀷"/>
        <when state="ᵐz" output="𐀼"/>
    </terminators>
</keyboard>
