        <when state="ᵍlenis" output="᾽"/>
        <when state="ᵍlenisAcute" output="῎"/>
        <when state="ᵍlenisAcuteIota" output="῎ͅ"/>
        <when state="ᵍlenisCircumflex" output="῏"/>
